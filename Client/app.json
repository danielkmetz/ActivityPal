{
  "expo": {
    "name": "ActivityPal",
    "slug": "activitypal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We use your camera to broadcast live video.",
        "NSMicrophoneUsageDescription": "We use your microphone to broadcast live audio.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.danielkmetz.activitypal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "52414b9a-292d-4649-a22b-f4ca020c1161"
      }
    },
    "owner": "danielkmetz",
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "We need access to your camera to let you create stories and record videos."
        }
      ]
    ]
  }
}
